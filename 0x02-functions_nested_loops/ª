#include "main.h"

/**
 * jack_bauer - Entry
 *
 * Return: 0
 */
void jack_bauer(void)
{
	int a;
	int b;
	int c;
	int d;

	a = 48;
	while (a <= 50)
	{
		b = 48;
		while (b <= 57)
		{
			c = 48;
			while (c <= 53)
			{
				d = 48;
				while (d <= 57)
				{
					if (a <= 50)
					{
						_putchar(a);
						_putchar(b);
						_putchar(':');
						_putchar(c);
						_putchar(d);
						_putchar(10);
						d++;
						if (a == 50 && b == 51 && c == 53 && d == 57)
						{
							break;
							_putchar(10);

						}	
					}
				}	
				c++;
			}
			b++;
		}
		a++;
	}
}
